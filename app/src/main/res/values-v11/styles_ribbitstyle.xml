<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

        <style name="Theme.Ribbitstyle" parent="@android:style/Theme.Holo.Light">
                <item name="android:actionBarItemBackground" tools:targetApi="ice_cream_sandwich">
                        @drawable/selectable_background_ribbitstyle
                </item>
                <item name="android:popupMenuStyle">@style/PopupMenu.Ribbitstyle</item>
                <item name="android:dropDownListViewStyle">@style/DropDownListView.Ribbitstyle
                </item>
                <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Ribbitstyle</item>
                <item name="android:actionDropDownStyle">@style/DropDownNav.Ribbitstyle</item>
                <item name="android:actionBarStyle">@style/ActionBar.Solid.Ribbitstyle</item>
                <item name="android:actionModeBackground">@drawable/cab_background_top_ribbitstyle
                </item>
                <item name="android:actionModeSplitBackground" tools:targetApi="ice_cream_sandwich">
                        @drawable/cab_background_bottom_ribbitstyle
                </item>
                <item name="android:actionModeCloseButtonStyle">
                        @style/ActionButton.CloseMode.Ribbitstyle
                </item>
                <item name="android:listViewStyle">@style/Theme.Ribbitstyle.LightPurpleBackground
                </item>


        </style>

        <style name="ActionBar.Transparent.Ribbitstyle" parent="@android:style/Widget.Holo.Light.ActionBar">
                <item name="android:background">@drawable/ab_transparent_ribbitstyle</item>
                <item name="android:progressBarStyle">@style/ProgressBar.Ribbitstyle</item>
        </style>

        <style name="PopupMenu.Ribbitstyle" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
                <item name="android:popupBackground">@drawable/menu_dropdown_panel_ribbitstyle
                </item>
        </style>

        <style name="DropDownListView.Ribbitstyle" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
                <item name="android:listSelector">@drawable/selectable_background_ribbitstyle</item>
        </style>

        <style name="DropDownNav.Ribbitstyle" parent="@android:style/Widget.Holo.Light.Spinner">
                <item name="android:background">@drawable/spinner_background_ab_ribbitstyle</item>
                <item name="android:popupBackground">@drawable/menu_dropdown_panel_ribbitstyle
                </item>
                <item name="android:dropDownSelector">@drawable/selectable_background_ribbitstyle
                </item>
        </style>

        <style name="ProgressBar.Ribbitstyle" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
                <item name="android:progressDrawable">@drawable/progress_horizontal_ribbitstyle
                </item>
        </style>

        <style name="ActionButton.CloseMode.Ribbitstyle" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
                <item name="android:background">@drawable/btn_cab_done_ribbitstyle</item>
        </style>

        <style name="Theme.Ribbitstyle.Widget" parent="@android:style/Theme.Holo">
                <item name="android:popupMenuStyle">@style/PopupMenu.Ribbitstyle</item>
                <item name="android:dropDownListViewStyle">@style/DropDownListView.Ribbitstyle
                </item>
        </style>
</resources>